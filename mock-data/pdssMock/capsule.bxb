capsule {
  id (bixby.pdssMock)
  version (0.1.0)
  format (3)
  targets {
    target (bixby-mobile-en-US)
  }
  runtime-version (2) {
    js-runtime-version (2)
    overrides {
      allow-dialogs-on-detail-pages (true)
      no-fallback-to-result-collections (true)
    }
  }
  store-countries: all
  capsule-imports {
    import (bixby.installedApps) {
      as (installedApps)
      version (1.1.5)
    }
    import (bixby.bhp) {
      version (2.108.6)
      as (smarthome)
    }
  }
}