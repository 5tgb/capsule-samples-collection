dialog (Value) {
  match: ProductType(this)
  switch (this) {
    case (FlowerArrangement) {
      template("Flower Arrangement")
    }
    case (Bouquet) {
      template("Bouquet")
    }
    case (Plantable) {
      template("Plantable")
    }
  }
}

