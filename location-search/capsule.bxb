capsule {
  id (example.locationSearch)
  version (1.0.2)
  format (3)
  targets {
    target (bixby-mobile-en-US)
  }
  capsule-imports {
    import (viv.location) { as (location) version (1.0.45)}
    import (viv.geo) { as (geo) version-from (location)}
  }
  runtime-version (7)
  store-sections {
    section (TravelAndTransportation)
  }
  store-countries {
    all
  }
}
